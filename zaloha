void create_process(void (*process_func) (int, FILE *),int element_num, int * 
number_of_type,int TI)
{
    if(*number_of_type == element_num)
    {
        process_func(TI,stdout);
        return;
    }
    pid_t ID = fork();
    if(ID == 0)
    {
        if(*number_of_type < element_num)//*no_o ked je proces NO a *no_h, ked je proces NH
        {
            create_process(process_func,element_num,number_of_type,TI);
        }
        process_func(element_num,stdout);
            while (wait(NULL) > 0);
    }
    else if(ID == -1)
    {
        error_exit("Fork error.");
    }
    else
    {
        process_func(element_num,stdout);
    }

}
